<?xml version="1.0"?>

#filter substitution
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">

    <!-- Songbird -->
    <em:targetApplication>
      <Description>
        <em:id>songbird@songbirdnest.com</em:id>
        <em:minVersion>1.1.1</em:minVersion>
        <em:maxVersion>1.2.0a</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:id>@EXTENSION_UUID@</em:id>
    <em:name>MacBook Extras</em:name>
    <em:description>
      Adds a few features to make Songbird more MacBook friendly
    </em:description>
    <em:creator>Steve Krulewitz</em:creator>
    <em:homepageURL>http://skrul.com</em:homepageURL>
    <em:version>1.0.1</em:version>

    <em:type>2</em:type>
    <em:targetPlatform>@EXTENSION_ARCH@</em:targetPlatform>

  </Description>

</RDF>
